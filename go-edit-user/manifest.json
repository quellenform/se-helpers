{
  "manifest_version": 3,
  "name": "Navigate to Stack Exchange Edit Profile",
  "version": "1.0",
  "description": "Chrome extension that when clicked goes to Edit Profile",
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ]
}
